<!DOCTYPE html>
<html>
  <head>
    <title>WhatsNext</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <nav>
      <h1>What's Next</h1>
      <% if current_user %>
        <h5><%= current_user.name %></h5>
      <% end %>
    </nav>
    <%= yield %>
  </body>
</html>
