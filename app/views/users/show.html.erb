<div class="likes-container">
  <%= render "users/user_likes" %>
  <%= form_tag do %>
    <%= label_tag :item_name %>
    <%= text_field_tag :item_name %>
    <%= select_tag :item_type, options_for_select([['music', 0], ['movie', 1], ['show', 2], ['podcast', 3], ['book', 4], ['author', 5], ['game', 6]]) %>
    <%= submit_tag "Add New" %>
  <% end %>
</div>
<%= render "recs_container" %>